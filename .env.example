# Sendblue API credentials
SENDBLUE_API_KEY=your_sendblue_api_key
SENDBLUE_API_SECRET=your_sendblue_api_secret

# Your phone number to receive notifications (E.164 format, e.g., +1234567890)
YOUR_PHONE_NUMBER=+1234567890

# Port (Cloud Run will set this automatically)
PORT=8080

# Node environment
NODE_ENV=production

# Optional: Custom Sendblue domain if different
# SENDBLUE_DOMAIN=https://api.sendblue.co